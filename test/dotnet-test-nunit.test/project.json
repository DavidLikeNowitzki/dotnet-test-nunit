{
  "copyright": "Copyright (C) 2016 Charlie Poole",
  "authors": [ "Rob Prouse" ],
  "title": "NUnit .NET Core Runner Tests",
  "name": "dotnet-test-nunit.test",
  "version": "3.3.0-*",
  "buildOptions": {
    "preserveCompilationContext": true,
    "warningsAsErrors": true
  },

  "dependencies": {
    "NUnit": "3.2.0",
    "dotnet-test-nunit": "3.3.0"
  },
  "testRunner": "nunit",

  "frameworks": {
    "netstandard1.5": {
      "imports": [
        "dnxcore50",
        "netstandard1.5",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-rc2-3002702",
          "type": "platform"
        }
      }
    },
    "net451": {
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1-rc2-24027"
      }
    }
  }
}
